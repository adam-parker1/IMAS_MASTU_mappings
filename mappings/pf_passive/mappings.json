{
    "ids_properties/homogeneous_time": {
        "MAP_TYPE": "VALUE",
        "VALUE": 0
    },
    "loop/Shape_of": {
        "MAP_TYPE": "VALUE",
        "VALUE": "{{ length(LOOPS) }}"
    },
    "loop[#]/resistance": {
        "MAP_TYPE": "PLUGIN",
        "PLUGIN": "GEOM",
        "ARGS": {
            "signal": "/passive/efit/{{ PASSIVE_NAME }}",
            "key": "resistance"
        }
    },
    "loop[#]/resistivity": {
        "MAP_TYPE": "PLUGIN",
        "PLUGIN": "GEOM",
        "ARGS": {
            "signal": "/passive/efit/{{ PASSIVE_NAME }}",
            "key": "resistivity"
        }
    },
    "loop[#]/current": {
        "MAP_TYPE": "PLUGIN",
        "PLUGIN": "UDA",
        "ARGS": {
            "signal": "{{ PASSIVE_CURRENT }}"
        },
        "SCALE": "{{ UNIT_SF }}"
    },
    "loop[#]/element/Shape_of": {
        "MAP_TYPE": "DIMENSION",
        "DIM_PROBE": "loop[#]/element[#]/geometry/oblique/r"
    },
    "loop[#]/element[#]/name": {
        "MAP_TYPE": "VALUE",
        "VALUE": "{{ PASSIVE_NAME }}_{{ indices.1 }}"
    },
    "loop[#]/element[#]/identifier": {
        "MAP_TYPE": "VALUE",
        "VALUE": "LOOP_ELEM_{{ indices.0 }}"
    },
    "loop[#]/element[#]/area": {
        "MAP_TYPE": "EXPR",
        "PARAMETERS": {
            "dR": "_dR",
            "dZ": "_dZ"
        },
        "EXPR": "dR*dZ"
    },
    "loop[#]/element[#]/geometry/geometry_type": {
        "MAP_TYPE": "VALUE",
        "VALUE": 3
    },
    "loop[#]/element[#]/geometry/oblique/r": {
        "MAP_TYPE": "PLUGIN",
        "PLUGIN": "GEOM",
        "ARGS": {
            "signal": "/passive/efit/{{ PASSIVE_NAME }}",
            "key": "centreR"
        }
    },
    "loop[#]/element[#]/geometry/oblique/z": {
        "MAP_TYPE": "PLUGIN",
        "PLUGIN": "GEOM",
        "ARGS": {
            "signal": "/passive/efit/{{ PASSIVE_NAME }}",
            "key": "centreZ"
        }
    },
    "loop[#]/element[#]/geometry/oblique/alpha": {
        "MAP_TYPE": "PLUGIN",
        "PLUGIN": "GEOM",
        "ARGS": {
            "signal": "/passive/efit/{{ PASSIVE_NAME }}",
            "key": "shapeAngle1"
        },
        "SCALE": "{{ DEG2RAD }}"
    },
    "loop[#]/element[#]/geometry/oblique/beta": {
        "MAP_TYPE": "PLUGIN",
        "PLUGIN": "GEOM",
        "ARGS": {
            "signal": "/passive/efit/{{ PASSIVE_NAME }}",
            "key": "shapeAngle2"
        },
        "OFFSET": 90
    },
    "loop[#]/element[#]/geometry/oblique/length_alpha": {
        "MAP_TYPE": "EXPR",
        "PARAMETERS": {
            "dR": "_dR",
            "a": "loop[#]/element[#]/geometry/oblique/alpha"
        },
        "EXPR": "dR / cos(a)"
    },
    "loop[#]/element[#]/geometry/oblique/length_beta": {
        "MAP_TYPE": "EXPR",
        "PARAMETERS": {
            "dZ": "_dZ",
            "b": "loop[#]/element[#]/geometry/oblique/beta"
        },
        "EXPR": "dZ / cos(b)"
    },
    "_dR": {
        "MAP_TYPE": "PLUGIN",
        "PLUGIN": "GEOM",
        "ARGS": {
            "signal": "/passive/efit/{{ PASSIVE_NAME }}",
            "key": "dR"
        }
    },
    "_dZ": {
        "MAP_TYPE": "PLUGIN",
        "PLUGIN": "GEOM",
        "ARGS": {
            "signal": "/passive/efit/{{ PASSIVE_NAME }}",
            "key": "dZ"
        }
    },
    "_shapeAngle2": {
        "MAP_TYPE": "PLUGIN",
        "PLUGIN": "GEOM",
        "ARGS": {
            "signal": "/passive/efit/{{ PASSIVE_NAME }}",
            "key": "shapeAngle2"
        },
        "SCALE": -1.0
    }

}
